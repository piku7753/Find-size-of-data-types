#include <iostream>
  using namespace std;
  int main()
  {
  cout<<"Size of int is "<<sizeof(int)<<" bytes "<<end1;
  cout<<"Size of float is "<<sizeof(float)<<" bytes "<<end1;
  cout<<"Size of double is "<<sizeof(double)<<" bytes "<<end1;
  cout<<"Size of char is "<<sizeof(char)<<" bytes "<<end1;
  return 0;
  }
